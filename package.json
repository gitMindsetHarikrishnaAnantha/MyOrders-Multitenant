{
  "name": "myorders",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.8.1",
    "@sap/cds": "^7.4.1",
    "@sap/cds-mtxs": "^1.13.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "express": "^4.18.2",
    "hdb": "^0.19.7",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/ux-specification": "^1.120.3"
  },
  "scripts": {
    "build": "mbt build",
    "deploy": "cf deploy --retries 0",
    "start": "cds-serve",
    "watch-myordersui": "cds watch --open myordersui/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "sapux": [
    "app/myordersui"
  ],
  "cds": {
    "hana": {
